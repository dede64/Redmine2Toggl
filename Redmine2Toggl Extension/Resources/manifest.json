{
  "manifest_version": 3,
  "name": "Redmine2Toggl",
  "version": "1.0.0",
  "description": "Copy current Redmine ticket as #ID: [Title]",
  "permissions": ["activeTab", "scripting", "contextMenus", "clipboardWrite", "tabs"],
  "action": { "default_title": "Copy Redmine Ticket" },
  "background": { "service_worker": "background.js" }
}
